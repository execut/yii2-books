{
  "name": "execut/yii2-books",
  "description": "Yii2 books module",
  "keywords": ["yii2 books"],
  "homepage": "https://github.com/execut/yii2-books",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/execut/yii2-books/issues?state=open",
    "wiki": "https://github.com/execut/yii2-books/wiki",
    "source": "https://github.com/execut/yii2-books"
  },
  "authors": [
    {
      "name": "eXeCUT"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "yiisoft/yii2": "@dev",
    "yiisoft/yii2-imagine": "*",
    "execut/yii2-crud": "*",
    "execut/yii2-crud-fields": "*",
    "execut/yii2-migration": "*"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "bootstrap": "execut\\books\\bootstrap\\Auto"
  },
  "autoload": {
    "psr-4": {
      "execut\\books\\": "src"
    }
  }
}